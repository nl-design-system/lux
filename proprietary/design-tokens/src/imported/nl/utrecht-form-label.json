{
  "utrecht": {
    "form-label": {
      "color": {
        "$type": "color",
        "$value": "{lux.color.foreground.default}"
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{lux.font-weight.bold}"
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{lux.font-size.body.default}"
      },
      "checkbox": {
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{lux.font-weight.regular}"
        },
        "color": {
          "$type": "color",
          "$value": "{lux.color.foreground.default}"
        }
      },
      "checked": {
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{lux.font-weight.regular}"
        }
      },
      "disabled": {
        "color": {
          "$type": "color",
          "$value": "{lux.color.foreground.subdued}"
        }
      },
      "radio": {
        "color": {
          "$type": "color",
          "$value": "{lux.color.foreground.default}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{lux.font-weight.regular}"
        }
      }
    }
  }
}