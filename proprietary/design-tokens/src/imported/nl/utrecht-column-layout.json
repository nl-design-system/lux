{
  "utrecht": {
    "column-layout": {
      "column-rule-color": {
        "$type": "color",
        "$value": "{lux.color.none}"
      },
      "column-rule-width": {
        "$type": "borderWidth",
        "$value": "{lux.border-width.none}"
      },
      "column-width": {
        "$type": "sizing",
        "$value": "12.5rem",
        "$description": "full-size, hoe stel ik dit in? "
      },
      "gap": {
        "$type": "spacing",
        "$value": "{utrecht.column-layout.column-gap}",
        "$description": "Deze token uitfaseren"
      },
      "column-gap": {
        "$type": "spacing",
        "$value": "{lux.grid.gutter}"
      }
    }
  }
}