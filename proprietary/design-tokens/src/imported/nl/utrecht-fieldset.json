{
  "utrecht": {
    "form-fieldset": {
      "legend": {
        "color": {
          "$type": "color",
          "$value": "{lux.color.brand.default}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{lux.font-family.primary}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{lux.font-size.heading.level-3}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{lux.font-weight.bold}"
        },
        "line-height": {
          "$type": "lineHeights",
          "$value": "{lux.line-height.heading.level-3}"
        },
        "disabled": {
          "color": {
            "$type": "color",
            "$value": "{lux.color.foreground.subdued}",
            "$description": "Do not use a disabled state"
          }
        }
      },
      "section": {
        "background-color": {
          "$type": "color",
          "$value": "{lux.color.none}"
        },
        "color": {
          "$type": "color",
          "$value": "{lux.color.foreground.default}"
        }
      },
      "invalid": {
        "border-inline-start": {
          "color": {
            "$type": "color",
            "$value": "{utrecht.form-field.invalid.border-inline-start-color}"
          }
        },
        "border-inline-start-width": {
          "$type": "borderWidth",
          "$value": "{utrecht.form-field.invalid.border-inline-start-width}"
        },
        "padding-inline-start": {
          "$type": "spacing",
          "$value": "{utrecht.form-field.invalid.padding-inline-start}"
        }
      }
    }
  }
}