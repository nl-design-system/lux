        "$value": "{lux.font-family.primary}"
      },
      "color": {
        "$type": "color",
        "$value": "{lux.color.foreground.default}"
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{lux.font-weight.regular}"
