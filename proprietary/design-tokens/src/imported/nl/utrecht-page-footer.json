{
  "utrecht": {
    "page-footer": {
      "background-color": {
        "$type": "color",
        "$value": "{lux.color.background.default}"
      },
      "color": {
        "$type": "color",
        "$value": "{lux.color.foreground.default}"
      },
      "padding-block-start": {
        "$type": "spacing",
        "$value": "{lux.space.600}"
      },
      "padding-block-end": {
        "$type": "spacing",
        "$value": "{lux.space.600}"
      },
      "padding-inline-start": {
        "$type": "spacing",
        "$value": "{lux.grid.margin}"
      },
      "padding-inline-end": {
        "$type": "spacing",
        "$value": "{lux.grid.margin}"
      },
      "border-color": {
        "$type": "color",
        "$value": "{lux.color.brand.default}"
      },
      "border-width": {
        "$type": "borderWidth",
        "$value": "{lux.border-width.m}"
      },
      "content": {
        "column-gap": {
          "$type": "spacing",
          "$value": "{lux.grid.gutter}"
        },
        "row-gap": {
          "$type": "spacing",
          "$value": "{lux.space.400}"
        },
        "column-width": {
          "$type": "sizing",
          "$value": "12.5rem"
        },
        "max-inline-size": {
          "$type": "sizing",
          "$value": "{lux.grid.max-inline-size}"
        }
      }
    }
  }
}