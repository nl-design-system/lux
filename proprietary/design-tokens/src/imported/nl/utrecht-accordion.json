{
  "utrecht": {
    "accordion": {
      "margin-block-end": {
        "$type": "dimension",
        "$value": "0px",
        "$description": "[code-only]"
      },
      "margin-block-start": {
        "$type": "dimension",
        "$value": "0px",
        "$description": "[code-only]"
      },
      "row-gap": {
        "$type": "dimension",
        "$value": "{lux.space.100}"
      },
      "button": {
        "background-color": {
          "$type": "color",
          "$value": "{lux.color.background.subdued}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{lux.color.none}"
        },
        "border-radius": {
          "$type": "dimension",
          "$value": "{lux.border-radius.default}"
        },
        "border-width": {
          "$type": "dimension",
          "$value": "{lux.border-width.default}"
        },
        "color": {
          "$type": "color",
          "$value": "{lux.color.foreground.default}"
        },
        "gap": {
          "$type": "dimension",
          "$value": "{lux.space.100}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{lux.space.150}"
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{lux.space.150}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "{lux.space.200}"
        },
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "{lux.space.200}"
        },
        "icon": {
