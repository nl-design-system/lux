{
  "version": "1.0.0-alpha.0",
  "author": "Community for NL Design System",
  "description": "Example design tokens",
  "license": "SEE LICENSE IN LICENSE.md",
  "name": "@lux/design-tokens",
  "keywords": [
    "nl-design-system"
  ],
  "_main": "dist/",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/lux.git"
  },
  "scripts": {
    "clean": "npm-run-all clean:**",
    "clean:dist": "rimraf dist/",
    "prebuild": "npm run clean",
    "build": "npm-run-all clean build:tokens-studio",
    "build:tokens-studio": "node studio.mjs",
    "build:style-dictionary": "node ./config.js",
    "watch": "npm-run-all watch:**",
    "watch:style-dictionary": "chokidar --follow-symlinks --initial --command 'npm run build' 'src/**/*.json'"
  },
  "devDependencies": {
    "@tokens-studio/sd-transforms": "0.15.2",
    "chokidar-cli": "3.0.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.5",
    "style-dictionary": "3.9.2",
    "token-transformer": "0.0.33"
  }
}
