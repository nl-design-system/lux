{
  "figma": {
    "rijkslint": {
      "variant": {
        "parent": "viewport (beta)/xs",
        "description": "",
        "$type": "text",
        "$value": "small"
      }
    },
    "screen-size": {
      "parent": "viewport (beta)/xs",
      "description": "",
      "$type": "sizing",
      "$value": "23.4375rem"
    },
    "viewport": {
      "size": {
        "icon": {
          "wrapper-height": {
            "parent": "viewport (beta)/xs",
            "$type": "sizing",
            "$value": "1.5rem"
          }
        }
      }
    }
  },
  "lux": {
    "grid": {
      "gutter": {
        "parent": "viewport (beta)/xs",
        "description": "",
        "$type": "spacing",
        "$value": "0rem"
      },
      "margin": {
        "parent": "viewport (beta)/xs",
        "description": "",
        "$type": "spacing",
        "$value": "1rem"
      }
    },
    "alert": {
      "icon": {
        "inset-block-start": {
          "$type": "spacing",
          "$value": "{lux.space.0}"
        }
      }
    }
  },
  "viewport": {
    "rijkslint": {
      "wordmark": {
        "75%": {
          "padding-block-start": {
            "parent": "viewport (beta)/xs",
            "description": "",
            "$type": "spacing",
            "$value": "1.25rem"
          }
        },
        "default": {
          "padding-block-start": {
            "parent": "viewport (beta)/xs",
            "description": "",
            "$type": "spacing",
            "$value": "1.25rem"
          }
        }
      }
    },
    "line-height": {
      "heading": {
        "level-1": {
          "parent": "viewport (beta)/xl",
          "$type": "lineHeights",
          "$value": "{lux.line-height.small-scale.xl}"
        },
        "level-2": {
          "parent": "viewport (beta)/xl",
          "$type": "lineHeights",
          "$value": "{lux.line-height.small-scale.lg}"
        },
        "level-3": {
          "parent": "viewport (beta)/xl",
          "$type": "lineHeights",
          "$value": "{lux.line-height.small-scale.md}"
        },
        "level-4": {
          "parent": "viewport (beta)/xl",
          "$type": "lineHeights",
          "$value": "{lux.line-height.small-scale.sm}"
        }
      },
      "body": {
        "default": {
          "$type": "lineHeights",
          "$value": "{lux.line-height.small-scale.sm}"
        },
        "large": {
          "$type": "lineHeights",
          "$value": "{lux.line-height.small-scale.md}"
        },
        "small": {
          "$type": "lineHeights",
          "$value": "{lux.line-height.small-scale.xs}"
        }
      }
    },
    "font-size": {
      "heading": {
        "level-1": {
          "parent": "viewport (beta)/xl",
          "$type": "fontSizes",
          "$value": "{lux.font-size.small-scale.xl}"
        },
        "level-2": {
          "parent": "viewport (beta)/xl",
          "$type": "fontSizes",
          "$value": "{lux.font-size.small-scale.lg}"
        },
        "level-3": {
          "parent": "viewport (beta)/xl",
          "$type": "fontSizes",
          "$value": "{lux.font-size.small-scale.md}"
        },
        "level-4": {
          "parent": "viewport (beta)/xl",
          "$type": "fontSizes",
          "$value": "{lux.font-size.small-scale.sm}"
        }
      },
      "body": {
        "default": {
          "$type": "fontSizes",
          "$value": "{lux.font-size.small-scale.sm}"
        },
        "large": {
          "$type": "fontSizes",
          "$value": "{lux.font-size.small-scale.md}"
        },
        "small": {
          "$type": "fontSizes",
          "$value": "{lux.font-size.small-scale.xs}"
        }
      }
    }
  }
}