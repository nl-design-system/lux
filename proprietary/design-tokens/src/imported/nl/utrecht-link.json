{
  "utrecht": {
    "link": {
      "color": {
        "$type": "color",
        "$value": "{lux.link.color}"
      },
      "text-decoration-color": {
        "$type": "color",
        "$value": "{lux.link.color}"
      },
      "text-decoration-thickness": {
        "$type": "other",
        "$value": "auto"
      },
      "text-underline-offset": {
        "$type": "other",
        "$value": "auto"
      },
      "active": {
        "color": {
          "$type": "color",
          "$value": "{lux.link.active.color}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "{lux.link.active.text-decoration}"
        }
      },
      "focus": {
        "color": {
          "$type": "color",
          "$value": "{lux.link.focus.color}"
        },
        "text-decoration-thickness": {
          "$type": "other",
          "$value": "auto"
        },
        "background-color": {
          "$type": "color",
          "$value": "{lux.link.focus.background-color}"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "{lux.link.focus.text-decoration}"
        }
      },
      "hover": {
        "color": {
          "$type": "color",
          "$value": "{lux.link.hover.color}"
        },
        "text-decoration-thickness": {
          "$type": "other",
          "$value": "auto"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "{lux.link.hover.text-decoration}"
        }
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{lux.link.font-weight}"
      },
      "visited": {
        "color": {
          "$type": "color",
          "$value": "{lux.link.visited.color}"
        }
      },
      "icon": {
        "size": {
          "$type": "sizing",
          "$value": "{lux.link.icon.size}"
        }
      },
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{lux.link.font-family}"
      },
      "line-height": {
        "$type": "lineHeights",
        "$value": "{lux.link.line-height}"
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{lux.link.font-size}"
      },
      "text-decoration": {
        "$type": "textDecoration",
        "$value": "{lux.link.text-decoration}"
      },
      "column-gap": {
        "$type": "spacing",
        "$value": "{lux.link.column-gap}"
      }
    }
  }
}