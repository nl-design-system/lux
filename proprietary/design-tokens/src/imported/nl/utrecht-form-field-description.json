        }
      },
      "valid": {
        "color": {
          "$type": "color",
          "$value": "{lux.color.foreground.subdued}",
          "$description": "Checkvraag dev: bestaat deze echt in code? "
        }
      }
    },
    "invalid": {
      "font-weight": {
