{
  "utrecht": {
    "focus": {
      "outline-color": {
        "$type": "color",
        "$value": "{lux.color.focus.outline}"
      },
      "outline-offset": {
        "$type": "spacing",
        "$value": "{lux.focus.outline-offset}"
      },
      "outline-style": {
        "$type": "other",
        "$value": "{lux.focus.outline.style}"
      },
      "outline-width": {
        "$type": "borderWidth",
        "$value": "{lux.focus.outline-width}"
      },
      "inverse": {
        "outline-color": {
          "$type": "color",
          "$value": "transparant"
        }
      }
    }
  }
}